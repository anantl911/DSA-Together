//{ Driver Code Starts
#include <bits/stdc++.h>
using namespace std;


// } Driver Code Ends
class Solution
{
    public:
    //Function is to check whether two strings are anagram of each other or not.
    bool isAnagram(string a, string b){
        int n1 = a.length(), n2 = b.length(), increment=0;
        if(n1==n2){
        sort(a.begin(), a.end());
        sort(b.begin(), b.end());
            for(int i = 0 ; i < n1 ; i++){
                    if(a[i]==b[i]) ++increment;
                    else continue;
            }
        }
        if(increment==n1) return true;
        else return false;
        
    }

};

//{ Driver Code Starts.

int main() {
    
    int t;

    cin >> t;

    while(t--){
        string c, d;

        cin >> c >> d;
        Solution obj;
        if(obj.isAnagram(c, d)) cout << "YES" << endl;
        else cout << "NO" << endl;
    }

}

// } Driver Code Ends
